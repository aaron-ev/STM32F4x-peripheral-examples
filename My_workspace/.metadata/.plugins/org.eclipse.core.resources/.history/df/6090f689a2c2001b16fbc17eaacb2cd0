#include "stm32F4xx_hal.h"
#include "main.h"
int main(void)
{
	HAL_Init();
	GPIO_init();
	while(1);
	return 0;
}

void GPIO_init()
{
	GPIO_InitTypeDef ledBoard = {0};

	ledBoard.Mode = GPIO_MODE_OUTPUT_PP;
	ledBoard.Pull = GPIO_PULLUP;
	ledBoard.Pin = GPIO_PIN_5;

	HAL_GPIO_Init(GPIOA,&ledBoard);
}
void Error_Handler(void)
{
	while(1);
}
